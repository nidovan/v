      },
      "applicationUrl": "http://localhost:5000",
      "sslPort": 0
    }
  }
}










services.AddCors(options =>
{
    options.AddDefaultPolicy(builder =>
    {
        builder.WithOrigins("http://your-react-app-domain.com")
               .AllowAnyHeader()
               .AllowAnyMethod();
    });
});


